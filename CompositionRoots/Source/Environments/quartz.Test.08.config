<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <!-- overwrite existing jobs -->
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>

    <!-- ************************************* -->
    <!-- Работа с локальной очередью сообщений -->
    <!-- ************************************* -->

    <!-- Обработка сообщений локальной очереди -->
    <job>
      <name>Обработка сообщений локальной очереди</name>
      <group>Erm</group>
      <description>Обработка сообщений локальной очереди</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.LocalMessages.ProcessLocalMessages, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>
	
    <trigger>
      <cron>
        <name>Обработка сообщений локальной очереди - каждую минуту</name>
        <group>Erm</group>
        <job-name>Обработка сообщений локальной очереди</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
        
      </cron>
    </trigger>

    <!-- Проверка локальной очереди на зависшие сообщения -->
    <job>
      <name>Проверка локальной очереди на зависшие сообщения</name>
      <group>Erm</group>
      <description>Проверка локальной очереди на зависшие сообщения</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.LocalMessages.ReprocessLocalMessages, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
      
      <!-- Число минут для определения зависших сообщений -->
      <job-data-map>
        <entry>
          <key>PeriodInMinutes</key>
          <value>60</value>
        </entry>
      </job-data-map>

    </job>
	
    <trigger>
      <cron>
        <name>Проверка локальной очереди на зависшие сообщения - каждую минуту</name>
        <group>Erm</group>
        <job-name>Проверка локальной очереди на зависшие сообщения</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
        
      </cron>
    </trigger>

    <!-- ************************* -->
    <!-- Работа с шиной интеграции -->
    <!-- ************************* -->

    <!-- Импорт объектов из шины интеграции -->
    <!-- Возможные значения для Flows: flowPhoneZones;flowRubrics;flowGeography;flowCards;flowCardsForERM;flowGeoClassifier;flowStickers;flowFinancialData1C;flowBilling -->
    <job>
      <name>Импорт объектов из шины интеграции</name>
      <group>Erm</group>
      <description>Импорт объектов из шины интеграции</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ImportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
        <entry>
          <key>Flows</key>
          <value>flowPhoneZones;flowRubrics;flowGeography;flowCards;flowCardsForERM;flowGeoClassifier;flowStickers;flowFinancialData1C;flowBilling</value>
        </entry>
        <entry>
          <key>PregeneratedIdsAmount</key>
          <value>3000</value>
        </entry>
      </job-data-map>

    </job>
	
    <trigger>
      <cron>
        <name>Импорт объектов из шины интеграции - каждые 10 секунд</name>
        <group>Erm</group>
        <job-name>Импорт объектов из шины интеграции</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0/10 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
        
      </cron>
    </trigger>

    <!-- Экспорт объектов в шину интеграции -->
    <!-- Возможные значения для Flows: flowOrders.DenialReason;flowOrders.AdvMaterial;flowOrders.Order;flowNomenclatures.NomenclatureElement;flowNomenclatures.NomenclatureElementRelation;flowPriceLists.PriceList;flowPriceLists.PriceListPosition;flowOrders.Invoice;flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch;flowFinancialData.LegalEntity;flowCardExtensions.CardCommercial;flowFinancialData.Client;MsCrm.HotClient -->

    <!-- Экспорт DenialReasons, РМ, Заказов, Invoic'ов -->
    <job>
      <name>Экспорт объектов в шину интеграции - Заказы и РМ</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - Заказы и РМ</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>100</value>
        </entry>
      </job-data-map>
      
    </job>

    <!-- Экспорт заказов -->
    <trigger>
      <cron>
        <name>Экспорт заказов в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Заказы и РМ</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.Order</value>
          </entry>
        </job-data-map>

        <cron-expression>0/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт Invoice -->
    <trigger>
      <cron>
        <name>Экспорт финансовых заказов в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Заказы и РМ</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.Invoice</value>
          </entry>
        </job-data-map>

        <cron-expression>10/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт DenialReasons -->
    <trigger>
      <cron>
        <name>Экспорт DenialReasons в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Заказы и РМ</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.DenialReason</value>
          </entry>
        </job-data-map>

        <cron-expression>20/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт РМ -->
    <trigger>
      <cron>
        <name>Экспорт РМ в шину интеграции - временно никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Заказы и РМ</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.AdvMaterial</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт ранее не валидных РМ и заказов -->
    <trigger>
      <cron>
        <name>Экспорт заказов и РМ в шину интеграции, ранее не прошедших валидацию - временно никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Заказы и РМ</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.DenialReason;flowOrders.AdvMaterial;flowOrders.Order;flowOrders.Invoice</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт юр.лиц и клиентов -->
    <job>
      <name>Экспорт объектов в шину интеграции - юр.лица и клиенты</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - юр.лица и клиенты</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>100</value>
        </entry>
      </job-data-map>
      
    </job>

    <!-- Экспорт юридических лиц -->
    <trigger>
      <cron>
        <name>Экспорт юр. лиц в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - юр.лица и клиенты</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.LegalEntity</value>
          </entry>
        </job-data-map>

        <cron-expression>0/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт клиентов в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - юр.лица и клиенты</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.Client</value>
          </entry>
        </job-data-map>

        <cron-expression>15/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт ранее не валидных юридических лиц и клиентов -->
    <trigger>
      <cron>
        <name>Экспорт клиентов и юр. лиц в шину интеграции, ранее не прошедших валидацию - временно никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - юр.лица и клиенты</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.LegalEntity;flowFinancialData.Client</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт тематик -->
    <job>
      <name>Экспорт объектов в шину интеграции - тематики</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - тематики</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>100</value>
        </entry>
      </job-data-map>
    </job>

    <!-- Экспорт тематик -->
    <trigger>
      <cron>
        <name>Экспорт тематик в шину интеграции - временно никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - тематики</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт ранее не валидных тематик -->
    <trigger>
      <cron>
        <name>Экспорт тематик в шину интеграции, ранее не прошедших валидацию - временно никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - тематики</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт номенклатур -->
    <job>
      <name>Экспорт объектов в шину интеграции - номенклатуры</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - номенклатуры</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowNomenclatures.NomenclatureElement;flowNomenclatures.NomenclatureElementRelation</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>100</value>
        </entry>
      </job-data-map>
      
    </job>

    <!-- Экспорт номенклатур -->
    <trigger>
      <cron>
        <name>Экспорт номенклатур в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - номенклатуры</job-name>
        <job-group>Erm</job-group>

        <job-data-map>

        </job-data-map>

        <cron-expression>0/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>
    
    <!-- Экспорт ранее не валидных номенклатур -->
    <trigger>
      <cron>
        <name>Экспорт номенклатур в шину интеграции, ранее не прошедших валидацию - никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - номенклатуры</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт прайс-листов -->
    <job>
      <name>Экспорт объектов в шину интеграции - прайс-листы</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - прайс-листы</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowPriceLists.PriceList;flowPriceLists.PriceListPosition</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>2000</value>
        </entry>
      </job-data-map>

    </job>

    <!-- Экспорт прайсов -->
    <trigger>
      <cron>
        <name>Экспорт прайсов в шину интеграции - каждые 30 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - прайс-листы</job-name>
        <job-group>Erm</job-group>

        <cron-expression>15/30 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт ранее не валидных прайс-листов -->
    <trigger>
      <cron>
        <name>Экспорт прайсов в шину интеграции, ранее не прошедших валидацию - никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - прайс-листы</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>
    
    <!-- Постинтеграционные активности -->
    <!-- Возможные значения для Activities: SyncFirmAddresses -->
    <job>
      <name>Постинтеграционные активности</name>
      <group>Erm</group>
      <description>Постинтеграционные активности</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.PostIntegrationActivities.PostIntegrationActivitiesWithFirmsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Activities</key>
          <value>SyncFirmAddresses</value>
        </entry>
      </job-data-map>
      
    </job>

    <trigger>
      <cron>
        <name>Постинтеграционные активности - никогда</name>
        <group>Erm</group>
        <job-name>Постинтеграционные активности</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
        
      </cron>
    </trigger>
    
    <!-- **************************************** -->
    <!-- Работа с перспективностью рекламодателей -->
    <!-- **************************************** -->

    <!-- Расчёт перспективности клиентов -->
    <job>
      <name>Расчёт перспективности клиентов</name>
      <group>Erm</group>
      <description>Расчёт перспективности клиентов</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.Olap.CalculateClientPromising, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>
	
    <trigger>
      <cron>
        <name>Расчёт перспективности клиентов - в 2 часа ночи каждый день</name>
        <group>Erm</group>
        <job-name>Расчёт перспективности клиентов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
        
      </cron>
    </trigger>
    
    <!-- Импорт перспективности фирм -->
    <job>
      <name>Импорт перспективности фирм</name>
      <group>Erm</group>
      <description>Импорт перспективности фирм</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.Olap.ImportFirmPromising, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>
	
    <trigger>
      <cron>
        <name>Импорт перспективности фирм - в полночь каждый день</name>
        <group>Erm</group>
        <job-name>Импорт перспективности фирм</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
        
      </cron>
    </trigger>
    
    <!-- *********************** -->
    <!-- Работа с заявками от ЛК -->
    <!-- *********************** -->

    <!-- Автоматическая обработка заявок от ЛК на продление заказов -->
    <job>
      <name>Автоматическая обработка заявок от ЛК на продление заказов</name>
      <group>Erm</group>
      <description>Автоматическая обработка заявок от ЛК на продление заказов</description>
