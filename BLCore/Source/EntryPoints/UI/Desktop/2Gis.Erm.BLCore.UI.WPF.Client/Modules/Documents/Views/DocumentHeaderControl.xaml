<UserControl x:Class="DoubleGis.Erm.BLCore.UI.WPF.Client.Modules.Documents.Views.DocumentHeaderControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" 
             d:DataContext="{x:Static blendability:DesignTimeDataContainer+Documents.DefaultCardDocument}" Background="Green">
    <Grid>
        <TextBlock Text="{Binding DocumentName}" Foreground="White"/>
    </Grid>
</UserControl>
