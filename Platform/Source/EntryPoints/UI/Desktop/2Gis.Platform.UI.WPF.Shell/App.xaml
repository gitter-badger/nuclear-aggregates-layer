<Application x:Class="DoubleGis.Platform.UI.WPF.Shell.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:resources="clr-namespace:DoubleGis.Platform.UI.WPF.Resources;assembly=2Gis.Platform.UI.WPF.Resources">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="{x:Static resources:DictionaryUrls.ModernUI}" />
                <ResourceDictionary Source="{x:Static resources:DictionaryUrls.ModernUILight}"/>
                <ResourceDictionary Source="{x:Static resources:DictionaryUrls.ModernWindow}"/>
                <ResourceDictionary Source="{x:Static resources:DictionaryUrls.Menu}"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
