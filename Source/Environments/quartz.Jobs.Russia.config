      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.Olap.ImportFirmPromising, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <job>
      <name>Автоматическая обработка заявок от ЛК на продление заказов</name>
      <group>Erm</group>
      <description>Автоматическая обработка заявок от ЛК на продление заказов</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.OrderProcessingRequest.ProlongateOrdersJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>Поздравление с днем рождения</name>
      <group>Erm</group>
      <description>Поздравление с днем рождения</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.RequestBirthdaysCongratulationJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>Экспорт объектов в шину интеграции - рассылка</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - рассылка</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowDeliveryData.LetterSendRequest</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>500</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

    <job>
      <name>Экспорт объектов в шину интеграции - рассылка</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - рассылка</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowDeliveryData.LetterSendRequest</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>500</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

  </schedule>
</job-scheduling-data>
