<?xml version="1.0" encoding="utf-8"?>
<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!-- replace application settings -->
  <appSettings>
    <add key="EnableNotifications" value="false" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="EnableCaching" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="EnableReplication" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="TargetEnvironment" value="Test" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="EnableIntegration" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="IntegrationApplicationName" value="ERM" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <!-- Резервный вариант интеграции с экспортом -->
    <add key="FtpExportSite" value="ftp://rnd-114.dvlp.2gis.local/erm/" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="FtpExportSiteUsername" value="ftpowner" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="FtpExportSitePassword" value="ftp" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    <add key="TargetEnvironmentName" value="Expl.Russia" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
  </appSettings>
  <!-- replace connection strings -->
  <system.serviceModel>
    <client>
      <!-- service bus broker -->
      <endpoint name="NetTcpBinding_IBrokerApiSender" address="net.tcp://uk-bus-test02.2gis.local:8053/BrokerApiService" xdt:Transform="SetAttributes" xdt:Locator="Match(name)">
        <identity>
          <servicePrincipalName value="host/uk-bus-test02.2gis.local" xdt:Transform="SetAttributes" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
  <system.web>
    <!-- enable asp.net custom errors -->
    <customErrors mode="On" xdt:Transform="SetAttributes" />
  </system.web>
  <system.webServer>
    <!-- disable directory browsing -->
    <directoryBrowse enabled="false" xdt:Transform="SetAttributes" />
  </system.webServer>
</configuration>