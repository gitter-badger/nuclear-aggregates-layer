		<!-- Экспорт РМ -->
		<trigger>
			<cron>
				<name>Экспорт РМ в шину интеграции - каждую минуту</name>
				<group>Erm</group>
				<job-name>Экспорт объектов в шину интеграции</job-name>
				<job-group>Erm</job-group>

				<job-data-map>
