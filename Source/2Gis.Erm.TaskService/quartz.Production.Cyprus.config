<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <!-- overwrite existing jobs -->
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

	<schedule>

		<!-- ************************* -->
		<!-- Работа с шиной интеграции -->
		<!-- ************************* -->

		<!-- Импорт объектов из шины интеграции -->
		<!-- Возможные значения для Flows: flowPhoneZones;flowRubrics;flowGeography;flowCards -->
		<job>
			<name>Импорт объектов из шины интеграции</name>
			<group>Erm</group>
			<description>Импорт объектов из шины интеграции</description>
			<job-type>DoubleGis.Erm.TaskService.Jobs.ServiceBus.ImportObjectsJob, 2Gis.Erm.TaskService</job-type>

			<durable>true</durable>
			<recover>false</recover>

			<job-data-map>
				<entry>
					<key>ErmUserImpersonateAs</key>
					<value>Integration</value>
				</entry>
				<entry>
					<key>Flows</key>
					<value>flowPhoneZones;flowRubrics;flowGeography;flowCards;flowGeoClassifier</value>
				</entry>
				<entry>
					<key>PregeneratedIdsAmount</key>
					<value>3000</value>
				</entry>
			</job-data-map>

		</job>
		<trigger>
			<cron>
				<name>Импорт объектов из шины интеграции - каждую минуту</name>
				<group>Erm</group>
				<job-name>Импорт объектов из шины интеграции</job-name>
				<job-group>Erm</job-group>

				<cron-expression>0 0/1 * * * ?</cron-expression>
				<time-zone>N. Central Asia Standard Time</time-zone>
			</cron>
		</trigger>

		<!-- Экспорт объектов в шину интеграции -->
		<!-- Возможные значения для Flows: flowCardExtensions.CardCommercial;flowOrders.AdvMaterial;flowOrders.Order;flowFinancialData.LegalEntity;flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch -->
		<job>
			<name>Экспорт объектов в шину интеграции</name>
			<group>Erm</group>
			<description>Экспорт объектов в шину интеграции</description>
			<job-type>DoubleGis.Erm.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.TaskService</job-type>

			<durable>true</durable>
			<recover>false</recover>
		</job>

		<!-- Экспорт доп. аттибутов адресов -->
		<trigger>
			<cron>
				<name>Экспорт доп. аттибутов адресов в шину интеграции - каждую минуту</name>
				<group>Erm</group>
				<job-name>Экспорт объектов в шину интеграции</job-name>
				<job-group>Erm</job-group>

				<job-data-map>
					<entry>
						<key>Flows</key>
						<value>flowCardExtensions.CardCommercial</value>
					</entry>
					<entry>
						<key>ExportInvalidObjects</key>
						<value>false</value>
					</entry>
				</job-data-map>

				<cron-expression>0 0/1 * * * ?</cron-expression>
				<time-zone>N. Central Asia Standard Time</time-zone>

			</cron>
		</trigger>

		<trigger>
			<cron>
				<name>Экспорт доп. аттибутов адресов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
				<group>Erm</group>
				<job-name>Экспорт объектов в шину интеграции</job-name>
				<job-group>Erm</job-group>

				<job-data-map>
					<entry>
						<key>Flows</key>
						<value>flowCardExtensions.CardCommercial</value>
					</entry>
					<entry>
						<key>ExportInvalidObjects</key>
						<value>true</value>
					</entry>
				</job-data-map>

				<cron-expression>0 0 0/2 * * ?</cron-expression>
				<time-zone>N. Central Asia Standard Time</time-zone>

			</cron>
		</trigger>

