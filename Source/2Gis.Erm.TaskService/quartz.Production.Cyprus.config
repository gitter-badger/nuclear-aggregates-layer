			</cron>
		</trigger>

		<trigger>
			<cron>
				<name>Экспорт заказов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
				<group>Erm</group>
				<job-name>Экспорт объектов в шину интеграции</job-name>
				<job-group>Erm</job-group>

				<job-data-map>
					<entry>
						<key>Flows</key>
						<value>flowOrders.Order</value>
