<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <!-- overwrite existing jobs -->
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>

    <!-- ************************************* -->
    <!-- Работа с локальной очередью сообщений -->
    <!-- ************************************* -->

    <!-- Обработка сообщений локальной очереди -->
    <job>
      <name>Обработка сообщений локальной очереди</name>
      <group>Erm</group>
      <description>Обработка сообщений локальной очереди</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.LocalMessages.ProcessLocalMessages, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Обработка сообщений локальной очереди - каждую минуту</name>
        <group>Erm</group>
        <job-name>Обработка сообщений локальной очереди</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Проверка локальной очереди на зависшие сообщения -->
    <job>
      <name>Проверка локальной очереди на зависшие сообщения</name>
      <group>Erm</group>
      <description>Проверка локальной очереди на зависшие сообщения</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.LocalMessages.ReprocessLocalMessages, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <!-- Число минут для определения зависших сообщений -->
      <job-data-map>
        <entry>
          <key>PeriodInMinutes</key>
          <value>60</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Проверка локальной очереди на зависшие сообщения - каждую минуту</name>
        <group>Erm</group>
        <job-name>Проверка локальной очереди на зависшие сообщения</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Экспорт локальных сообщений -->
    <job>
      <name>Экспорт локальных сообщений</name>
      <group>Erm</group>
      <description>Экспорт локальных сообщений</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.LocalMessages.ExportLocalMessages, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Экспорт локальных сообщений - в полночь каждый день</name>
        <group>Erm</group>
        <job-name>Экспорт локальных сообщений</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- ************************* -->
    <!-- Работа с шиной интеграции -->
    <!-- ************************* -->

    <!-- Импорт объектов из шины интеграции -->
    <!-- Возможные значения для Flows: flowPhoneZones;flowRubrics;flowGeography;flowCards;flowGeoClassifier;flowStickers;flowFinancialData1C;flowBilling -->
    <job>
      <name>Импорт объектов из шины интеграции</name>
      <group>Erm</group>
      <description>Импорт объектов из шины интеграции</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ImportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
        <entry>
          <key>Flows</key>
          <value>flowPhoneZones;flowRubrics;flowGeography;flowCards;flowGeoClassifier;flowStickers;flowFinancialData1C;flowBilling</value>
        </entry>
        <entry>
          <key>PregeneratedIdsAmount</key>
          <value>3000</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Импорт объектов из шины интеграции - каждую минуту</name>
        <group>Erm</group>
        <job-name>Импорт объектов из шины интеграции</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Экспорт объектов в шину интеграции -->
    <!-- Возможные значения для Flows: flowOrders.DenialReason;flowOrders.AdvMaterial;flowOrders.Order;flowNomenclatures.NomenclatureElement;flowNomenclatures.NomenclatureElementRelation;flowPriceLists.PriceList;flowPriceLists.PriceListPosition;flowOrders.Invoice;flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch;flowFinancialData.LegalEntity;flowCardExtensions.CardCommercial;flowFinancialData.Client;MsCrm.HotClient -->

    <!-- Экспорт производственных объектов -->
    <job>
      <name>Экспорт объектов в шину интеграции - Производственные</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - Производственные</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowOrders.DenialReason;flowOrders.AdvMaterial;flowFinancialData.LegalEntity;flowOrders.Order</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>500</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <cron>
        <name>Экспорт объектов в шину интеграции - каждые 20 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Производственные</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0/20 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт объектов в шину интеграции, ранее не прошедших валидацию-->
    <trigger>
      <cron>
        <name>Экспорт объектов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Производственные</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.DenialReason;flowOrders.AdvMaterial;flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch;flowFinancialData.LegalEntity;flowOrders.Order;flowNomenclatures.NomenclatureElement;flowNomenclatures.NomenclatureElementRelation;flowPriceLists.PriceList;flowPriceLists.PriceListPosition;flowOrders.Invoice;flowCardExtensions.CardCommercial;flowFinancialData.Client</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Экспорт финансовых объектов -->
    <job>
      <name>Экспорт объектов в шину интеграции - Финансовые</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - Финансовые</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowNomenclatures.NomenclatureElement;flowNomenclatures.NomenclatureElementRelation;flowPriceLists.PriceList;flowPriceLists.PriceListPosition;flowOrders.Invoice;flowFinancialData.Client</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>500</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <cron>
        <name>Экспорт объектов в шину интеграции - каждую минуту</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Финансовые</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт тематик -->
    <job>
      <name>Экспорт объектов в шину интеграции - Тематики</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции - Тематики</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>200</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <cron>
        <name>Экспорт объектов, связанных с тематиками, в шину интеграции - каждые 20 минут</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции - Тематики</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/20 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт горячих клиентов в MsCrm -->
    <job>
      <name>Экспорт горячих клиентов в MsCrm</name>
      <group>Erm</group>
      <description>Экспорт горячих клиентов в MsCrm</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Flows</key>
          <value>MsCrm.HotClient</value>
        </entry>
        <entry>
          <key>ExportInvalidObjects</key>
          <value>false</value>
        </entry>
        <entry>
          <key>OperationCount</key>
          <value>30</value>
        </entry>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <cron>
        <name>Экспорт горячих клиентов в MsCrm - никогда</name>
        <group>Erm</group>
        <job-name>Экспорт горячих клиентов в MsCrm</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт горячих клиентов в MsCrm, ранее не прошедших валидацию - каждую минуту</name>
        <group>Erm</group>
        <job-name>Экспорт горячих клиентов в MsCrm</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0 1 1 ? 2099</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Постинтеграционные активности -->
    <!-- Возможные значения для Activities: SyncFirmAddresses -->
    <job>
      <name>Постинтеграционные активности</name>
      <group>Erm</group>
      <description>Постинтеграционные активности</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.PostIntegrationActivities.PostIntegrationActivitiesWithFirmsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Activities</key>
          <value>SyncFirmAddresses</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <cron>
        <name>Постинтеграционные активности - каждую минуту</name>
        <group>Erm</group>
        <job-name>Постинтеграционные активности</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- ***************** -->
    <!-- Работа с RabbitMQ -->
    <!-- ***************** -->

    <!-- Импорт сообщений из RabbitMQ -->
    <job>
      <name>Импорт сообщений из RabbitMQ</name>
      <group>Erm</group>
      <description>Импорт сообщений из RabbitMQ</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.RabbitMq.ImportLocalMessagesFromRabbitMq, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Импорт сообщений из RabbitMQ - каждую минуту</name>
        <group>Erm</group>
        <job-name>Импорт сообщений из RabbitMQ</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- **************************************** -->
    <!-- Работа с перспективностью рекламодателей -->
    <!-- **************************************** -->

    <!-- Расчёт перспективности клиентов -->
    <job>
      <name>Расчёт перспективности клиентов</name>
      <group>Erm</group>
      <description>Расчёт перспективности клиентов</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.Olap.CalculateClientPromising, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Расчёт перспективности клиентов - в 2 часа ночи каждый день</name>
        <group>Erm</group>
        <job-name>Расчёт перспективности клиентов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Импорт перспективности фирм -->
    <job>
      <name>Импорт перспективности фирм</name>
      <group>Erm</group>
      <description>Импорт перспективности фирм</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.Olap.ImportFirmPromising, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Импорт перспективности фирм - в полночь каждый день</name>
        <group>Erm</group>
        <job-name>Импорт перспективности фирм</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- *********************** -->
    <!-- Работа с заявками от ЛК -->
    <!-- *********************** -->

    <!-- Автоматическая обработка заявок от ЛК на продление заказов -->
    <job>
      <name>Автоматическая обработка заявок от ЛК на продление заказов</name>
      <group>Erm</group>
      <description>Автоматическая обработка заявок от ЛК на продление заказов</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.OrderProcessingRequest.ProlongateOrdersJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Автоматическая обработка заявок от ЛК на продление заказов - раз в 15 минут</name>
        <group>Erm</group>
        <job-name>Автоматическая обработка заявок от ЛК на продление заказов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/15 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Удаление висячих файлов -->
    <job>
      <name>Удаление висячих файлов</name>
      <group>Erm</group>
      <description>Удаление висячих файлов</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.DeleteOrphanFilesJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>Удаление висячих файлов - в час ночи каждый день</name>
        <group>Erm</group>
        <job-name>Удаление висячих файлов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 1 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Чистка логов -->
    <job>
      <name>Чистка логов</name>
      <group>Erm</group>
      <description>Чистка логов</description>
      <job-type>DoubleGis.Erm.BLCore.TaskService.Jobs.CleanupLogsJob, 2Gis.Erm.BLCore.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>Чистка логов - в шабат</name>
        <group>Erm</group>
        <job-name>Чистка логов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 11 ? * SAT</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

  </schedule>
</job-scheduling-data>