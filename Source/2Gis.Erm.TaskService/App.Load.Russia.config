<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <!-- replace application settings -->
  <appSettings>
    <add key="EnableReplication" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="EnableCaching" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="EnableRabbitMqQueue" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="EnableIntegration" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="IntegrationApplicationName" value="ERMLOADRUSSIA" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="EnableNotifications" value="false" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    <add key="TargetEnvironment" value="Test" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>

		<add key="RegionalTerritoryLocaleSpecificWord" value="Региональная территория" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>

    <add key="MaxWorkingThreads" value="5" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>

  <system.serviceModel>
    <client>
       <endpoint name="WsHttpBinding_IWithdrawalInfoApplicationService"
            address="https://money-distribution.api.load.erm.russia/WithdrawalInfo.svc"
            xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
      
      <endpoint name="WSHttpBinding_IIdentityProviderApplicationService_Secure"
          address="https://metadata.api.load.erm.russia/Identity.svc/Soap"
          xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    </client>
  </system.serviceModel>
</configuration>