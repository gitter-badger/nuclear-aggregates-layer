<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">

  <!-- overwrite existing jobs -->
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>

    <!-- ************************************* -->
    <!-- Работа с локальной очередью сообщений -->
    <!-- ************************************* -->

    <!-- Обработка сообщений локальной очереди -->
    <job>
      <name>Обработка сообщений локальной очереди</name>
      <group>Erm</group>
      <description>Обработка сообщений локальной очереди</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.LocalMessages.ProcessLocalMessages, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Обработка сообщений локальной очереди - каждую минуту</name>
        <group>Erm</group>
        <job-name>Обработка сообщений локальной очереди</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Проверка локальной очереди на зависшие сообщения -->
    <job>
      <name>Проверка локальной очереди на зависшие сообщения</name>
      <group>Erm</group>
      <description>Проверка локальной очереди на зависшие сообщения</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.LocalMessages.ReprocessLocalMessages, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <!-- Число минут для определения зависших сообщений -->
      <job-data-map>
        <entry>
          <key>PeriodInMinutes</key>
          <value>60</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Проверка локальной очереди на зависшие сообщения - каждую минуту</name>
        <group>Erm</group>
        <job-name>Проверка локальной очереди на зависшие сообщения</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/1 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Экспорт локальных сообщений -->
    <job>
      <name>Экспорт локальных сообщений</name>
      <group>Erm</group>
      <description>Экспорт локальных сообщений</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.LocalMessages.ExportLocalMessages, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Экспорт локальных сообщений - в полночь каждый день</name>
        <group>Erm</group>
        <job-name>Экспорт локальных сообщений</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- ************************* -->
    <!-- Работа с шиной интеграции -->
    <!-- ************************* -->

    <!-- Импорт объектов из шины интеграции -->
    <!-- Возможные значения для Flows: flowPhoneZones;flowRubrics;flowGeography;flowCards;flowCardsForERM;flowGeoClassifier;flowStickers;flowFinancialData1C -->
    <job>
      <name>Импорт объектов из шины интеграции</name>
      <group>Erm</group>
      <description>Импорт объектов из шины интеграции</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.ServiceBus.ImportObjectsJob, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
        <entry>
          <key>Flows</key>
          <value>flowPhoneZones;flowRubrics;flowGeography;flowCards;flowCardsForERM;flowGeoClassifier;flowStickers;flowFinancialData1C</value>
        </entry>
        <entry>
          <key>PregeneratedIdsAmount</key>
          <value>3000</value>
        </entry>
      </job-data-map>

    </job>
    <trigger>
      <cron>
        <name>Импорт объектов из шины интеграции - каждую минуту</name>
        <group>Erm</group>
        <job-name>Импорт объектов из шины интеграции</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0/10 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Экспорт объектов в шину интеграции -->
    <!-- Возможные значения для Flows: flowCardExtensions.CardCommercial;flowOrders.AdvMaterial;flowOrders.Order;flowFinancialData.LegalEntity;flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch;flowFinancialData.Client -->
    <job>
      <name>Экспорт объектов в шину интеграции</name>
      <group>Erm</group>
      <description>Экспорт объектов в шину интеграции</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.ServiceBus.ExportObjectsJob, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>EntityCount</key>
          <value>300</value>
        </entry>
      </job-data-map>
    </job>

    <!-- Экспорт клиентов -->
    <trigger>
      <cron>
        <name>Экспорт клиентов в шину интеграции - каждые 5 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.Client</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>100</value>
          </entry>
        </job-data-map>

        <cron-expression>0/5 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт клиентов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.Client</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт доп. аттибутов адресов -->
    <trigger>
      <cron>
        <name>Экспорт доп. аттибутов адресов в шину интеграции - каждые 10 минут</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowCardExtensions.CardCommercial</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>100</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0/10 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт доп. аттибутов адресов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowCardExtensions.CardCommercial</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт тематик -->
    <trigger>
      <cron>
        <name>Экспорт тематик в шину интеграции - каждые 10 минут</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>100</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0/10 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт тематик в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.Resource;flowOrders.Theme;flowOrders.ThemeBranch</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт заказов -->
    <trigger>
      <cron>
        <name>Экспорт заказов в шину интеграции - каждые 10 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.Order</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>100</value>
          </entry>
        </job-data-map>

        <cron-expression>0/10 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт заказов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.Order</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт юр лиц клиентов -->
    <trigger>
      <cron>
        <name>Экспорт юр лиц клиентов в шину интеграции - каждые 5 секунд</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.LegalEntity</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>100</value>
          </entry>
        </job-data-map>

        <cron-expression>0/5 * * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт юр лиц клиентов в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowFinancialData.LegalEntity</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт РМ -->
    <trigger>
      <cron>
        <name>Экспорт РМ в шину интеграции - каждые 10 минут</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.AdvMaterial</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>100</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0/10 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт РМ в шину интеграции, ранее не прошедших валидацию - каждые 2 часа</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>flowOrders.AdvMaterial</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Экспорт горячих клиентов в MsCrm -->
    <trigger>
      <cron>
        <name>Экспорт горячих клиентов в MsCrm - никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>mscrm.hotclient</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>false</value>
          </entry>
          <entry>
            <key>OperationCount</key>
            <value>30</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 5 31 2 ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Экспорт горячих клиентов в MsCrm, ранее не прошедших валидацию - никогда</name>
        <group>Erm</group>
        <job-name>Экспорт объектов в шину интеграции</job-name>
        <job-group>Erm</job-group>

        <job-data-map>
          <entry>
            <key>Flows</key>
            <value>mscrm.hotclient</value>
          </entry>
          <entry>
            <key>ExportInvalidObjects</key>
            <value>true</value>
          </entry>
        </job-data-map>

        <cron-expression>0 0 5 31 2 ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>

      </cron>
    </trigger>

    <!-- Постинтеграционные активности -->
    <!-- Возможные значения для Activities: SyncFirmAddresses -->
    <job>
      <name>Постинтеграционные активности</name>
      <group>Erm</group>
      <description>Постинтеграционные активности</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.PostIntegrationActivities.PostIntegrationActivitiesWithFirmsJob, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>Activities</key>
          <value>SyncFirmAddresses</value>
        </entry>
      </job-data-map>
    </job>

    <trigger>
      <cron>
        <name>Постинтеграционные активности - никогда</name>
        <group>Erm</group>
        <job-name>Постинтеграционные активности</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 5 31 2 ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- ***************** -->
    <!-- Работа с RabbitMQ -->
    <!-- ***************** -->

    <!-- Импорт сообщений из RabbitMQ -->
    <job>
      <name>Импорт сообщений из RabbitMQ</name>
      <group>Erm</group>
      <description>Импорт сообщений из RabbitMQ</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.RabbitMq.ImportLocalMessagesFromRabbitMq, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Импорт сообщений из RabbitMQ - никогда</name>
        <group>Erm</group>
        <job-name>Импорт сообщений из RabbitMQ</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 5 31 2 ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- **************************************** -->
    <!-- Работа с перспективностью рекламодателей -->
    <!-- **************************************** -->

    <!-- Расчёт перспективности клиентов -->
    <job>
      <name>Расчёт перспективности клиентов</name>
      <group>Erm</group>
      <description>Расчёт перспективности клиентов</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.Olap.CalculateClientPromising, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Расчёт перспективности клиентов - в 2 часа ночи каждый день</name>
        <group>Erm</group>
        <job-name>Расчёт перспективности клиентов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 2 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- Импорт перспективности фирм -->
    <job>
      <name>Импорт перспективности фирм</name>
      <group>Erm</group>
      <description>Импорт перспективности фирм</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.Olap.ImportFirmPromising, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>

      <job-data-map>
        <entry>
          <key>ErmUserImpersonateAs</key>
          <value>Integration</value>
        </entry>
      </job-data-map>

    </job>

    <trigger>
      <cron>
        <name>Импорт перспективности фирм - в полночь каждый день</name>
        <group>Erm</group>
        <job-name>Импорт перспективности фирм</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0 0 * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

    <!-- *********************** -->
    <!-- Работа с заявками от ЛК -->
    <!-- *********************** -->

    <!-- Автоматическая обработка заявок от ЛК на продление заказов -->
    <job>
      <name>Автоматическая обработка заявок от ЛК на продление заказов</name>
      <group>Erm</group>
      <description>Автоматическая обработка заявок от ЛК на продление заказов</description>
      <job-type>DoubleGis.Erm.TaskService.Jobs.OrderProcessingRequest.ProlongateOrdersJob, 2Gis.Erm.TaskService</job-type>

      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Автоматическая обработка заявок от ЛК на продление заказов - раз в 15 минут</name>
        <group>Erm</group>
        <job-name>Автоматическая обработка заявок от ЛК на продление заказов</job-name>
        <job-group>Erm</job-group>

        <cron-expression>0 0/15 * * * ?</cron-expression>
        <time-zone>N. Central Asia Standard Time</time-zone>
      </cron>
    </trigger>

